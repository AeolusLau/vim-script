{
    "coc.preferences.semanticTokensHighlights": false,
    "coc.preferences.colorSupport": true,
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.jumpCommand": "drop",
    "coc.preferences.silentAutoupdate": true,
    "coc.preferences.snippetStatusText": "SNIP",
    "clangd.path": "/usr/local/opt/llvm/bin/clangd",
    "clangd.semanticHighlighting": true,
    "diagnostic.displayByAle": true,
    "semanticTokens.filetypes": ["*"],
    "snippets.extends": {
        "cpp": ["c"]
    },
    "cSpell.userWords": [
      "azeroth",
      "chrono",
      "clangd",
      "cronet",
      "deserialization",
      "env",
      "imdemo",
      "imei",
      "imsdk",
      "iovecs",
      "jboolean",
      "jbyte",
      "jclass",
      "jint",
      "jlong",
      "jmethod",
      "jni",
      "jobject",
      "klink",
      "kuaishou",
      "kwai",
      "kwailink",
      "lichao",
      "liulichao",
      "llvm",
      "logcat",
      "mutex",
      "nolint",
      "quic",
      "relogin",
      "spdy",
      "thiz",
      "wifi",
      "xquic"
    ],
    "cSpell.ignoreWords": [
      "clazz",
      "huidu",
      "ljava"
    ]
}
